    "react-native-size-matters": "^0.4.0",
    "react-native-svg": "^13.9.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-view-shot": "^3.6.0",
    "react-native-vision-camera": "^2.15.4",
    "react-redux": "^8.0.5",
    "styled-components": "^5.3.11",
